<Window x:Class="PL.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="UserWindow" Height="427" Width="456">
    <Grid>
        <Label x:Name="Idlabel" Content="Id:" HorizontalAlignment="Left" Margin="131,110,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Passwordlabel1" Content="Password:" HorizontalAlignment="Left" Margin="131,179,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Id" HorizontalAlignment="Left" Margin="221,114,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Password" HorizontalAlignment="Left" Margin="221,183,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Loginbutton" Content="Log in" HorizontalAlignment="Left" Margin="190,237,0,0" VerticalAlignment="Top" Width="64" Click="Loginbutton_Click" Background="{StaticResource MyButtonBackground}"/>
        <Button x:Name="NewUserbutton" Content="Create a new User" HorizontalAlignment="Left" Margin="172,284,0,0" VerticalAlignment="Top" Width="109" Click="NewUserbutton_Click" Background="{StaticResource MyButtonBackground}"/>

    </Grid>
</Window>
