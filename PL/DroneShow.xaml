<Window x:Class="PL.DroneShow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="DroneShow" Height="450" Width="800">
    <Grid Margin="0,0,0,-6">
        <Label x:Name="label" Content="Id" Margin="48,20,655,376"/>
        <Label x:Name="label1" Content="model" Margin="48,307,655,89"/>
        <Label x:Name="label2" Content="Battery" Margin="48,253,655,143"/>
        <Label x:Name="label3" Content="Maximum Weight" Margin="48,351,641,45"/>
        <Label x:Name="label4" Content="Status" Margin="48,62,655,334"/>
        <Label x:Name="label5" Content="Delivery" Margin="49,111,654,285"/>
        <Label x:Name="label6" Content="Latitude" Margin="49,160,654,236"/>
        <Label x:Name="label7" Content="Lomgtitude" Margin="49,209,653,187" RenderTransformOrigin="0,0"/>
        <ComboBox x:Name="WeightSelector" HorizontalAlignment="Left" Margin="194,307,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Margin="194,164,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.249,0.144"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Margin="194,213,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
