        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="194,24,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="194,115,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.202,0.976"/>
        <Button x:Name="button" Content="Add" HorizontalAlignment="Left" Margin="575,62,0,0" VerticalAlignment="Top" Width="100" Click="button_Click"/>

    </Grid>
</Window>
